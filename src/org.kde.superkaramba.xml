<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.SuperKaramba">
    <method name="closeTheme">
      <arg type="b" direction="out"/>
      <arg name="prettyThemeName" type="s" direction="in"/>
    </method>
    <method name="hideSystemTray">
      <arg name="hide" type="b" direction="in"/>
    </method>
    <method name="openNamedTheme">
      <arg name="file" type="s" direction="in"/>
      <arg name="themeName" type="s" direction="in"/>
      <arg name="subTheme" type="b" direction="in"/>
    </method>
    <method name="openTheme">
      <arg name="file" type="s" direction="in"/>
    </method>
    <method name="quitSuperKaramba">
    </method>
    <method name="setIncomingData">
      <arg type="b" direction="out"/>
      <arg name="prettyThemeName" type="s" direction="in"/>
      <arg name="data" type="s" direction="in"/>
    </method>
    <method name="showThemeDialog">
    </method>
    <method name="themeNotify">
      <arg type="b" direction="out"/>
      <arg name="prettyThemeName" type="s" direction="in"/>
      <arg name="data" type="s" direction="in"/>
    </method>
  </interface>
</node>
